/* Menu-bar */
#menu-bar {
	clear:both;
	min-height:32px;
	line-height:32px;
	padding:0;
	z-index:100;
/* Needed so Superfish menus appear over flash & slideshows in IE7 */
	position:relative;
	color: #333333;
    font-family: "Palatino Linotype",Palatino,"Book Antiqua",Georgia,serif;
    font-size: 15px;
    font-weight: bold;
/*    line-height: 11px;*/
    margin-bottom: 1px;
    margin-left: 3px;
    margin-right: 3px;
    margin-top: 3px;
}

.region-secondary-content {
/*	float:left;*/
