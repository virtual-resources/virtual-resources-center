    font-family: "Palatino Linotype",Palatino,"Book Antiqua",Georgia,serif;
}

#menu-bar ul {
	padding:0 15px
}

#menu-bar ul ul {
	padding:0
}

#menu-bar ul,#menu-bar ul li {
	margin:0;
	list-style:none;
	list-style-image:none
}

#menu-bar ul li {
	float:left;
	padding:0
