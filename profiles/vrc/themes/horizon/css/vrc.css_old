	color:/* Tweak this to suit your color scheme */
#fff!important
}

ul.sf-menu.sf-vertical,ul.sf-menu.sf-vertical ul,ul.sf-menu.sf-vertical ul ul,ul.sf-menu.sf-vertical ul ul ul {
	padding:0;
	margin:0
}

.nav .block-content {
	padding:5px 0 12px;
	margin:0
}

ul.sf-menu.sf-vertical,ul.sf-menu.sf-vertical li {
