}

/* Prevent overflowing content */
#content,.nav,#sidebar-first,#sidebar-second,#footer {
	overflow:visible;
	word-wrap:/* A very nice CSS3 property */
break-word
}

/* Pager */
ul.pager {
	padding:5px 0;
	margin:0
}

div.item-list ul.pager li {
