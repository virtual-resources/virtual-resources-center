	padding:2px 10px;
	color:#666;
	font-weight:400;
	font-size:1em;
	border:solid 1px #888;
	background:#fff url(../images/button.gif) repeat-x center
}

.form-submit:hover {
	color:#000;
	border:solid 1px #666;
	background:#fff url(../images/button-o.gif) repeat-x center
}

/* Local task tabs */
